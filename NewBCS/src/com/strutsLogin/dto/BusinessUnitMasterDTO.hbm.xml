<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.strutsLogin.dto">
	<class name="BusinessUnitMasterDTO" table="BusinessUnitMasterTbl">
		<id name="businessUnitId" column="BusinessUnitId">
			<generator class="increment"></generator>
		</id>
		
		
		<property name="businessUnitName" column="BusinessUnitName"/>

</class>
</hibernate-mapping>
