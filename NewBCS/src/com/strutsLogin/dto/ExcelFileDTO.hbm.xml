<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.strutsLogin.dto">
	<class name="ExcelFileDTO" table="excelfile">
		
		<id name="id" column="id">
			<generator class="increment"></generator>
		</id>
		
		<property name="employeeId" column="empId"></property>		
		<property name="name" column="empName"/>
		<property name="salary" column="empSalary"/>	

	</class>
</hibernate-mapping>
