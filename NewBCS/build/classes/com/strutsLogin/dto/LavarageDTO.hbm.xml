<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.strutsLogin.dto">
	<class name="LavarageDTO" table="LeverageTbl">
		<id name="leverageId" column="LeverageId">
			<generator class="increment"></generator>
		</id>
		
		
		<property name="businessUnitId" column="BusinessUnitId"/>
		<property name="gradeName" column="GradeName"/>
		<property name="countryId" column="CountryId"/>
		<property name="leveragePerc" column="LeveragePerc"/>

</class>
</hibernate-mapping>
